{
    "docdash": {
        "menu": {
            "Project Top": {
                "class": "menu-item",
                "href": "https://ms0503.github.io/interticle",
                "id": "website_link",
                "target": "_blank"
            }
        },
        "openGraph": {
            "site_name": "Interticle API Documentation",
            "type": "website",
            "url": "https://ms0503.github.io/interticle/apidoc"
        },
        "private": false,
        "search": true,
        "sectionOrder": [
            "Classes",
            "Modules",
            "Externals",
            "Namespaces",
            "Events",
            "Mixins",
            "Tutorials",
            "Interfaces"
        ],
        "sort": true,
        "static": true,
        "typedefs": false
    },
    "opts": {
        "destination": "./docs/apidoc/",
        "encoding": "utf8",
        "readme": "./README.md",
        "recurse": true,
        "template": "node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "includePattern": "src/.+\\.ts$"
    },
    "tags": {
        "allowUnknownTags": false
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
